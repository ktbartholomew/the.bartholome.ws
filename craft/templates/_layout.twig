{% cache globally using key craft.request.path unless craft.config.devMode %}<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="en-US">
<head>
  <meta charset="utf-8" />
  <title>{% block metaTitle %}{{ seo.metaTitle }}{% endblock %}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no"/>
  <meta name="description" content="{% block metaDescription %}{{ seo.metaDescription }}{% endblock %}"/>
  <meta name="og:title" content="{% block openGraphTitle %}{{ seo.openGraphTitle }}{% endblock %}"/>
  <meta name="og:description" content="{% block openGraphDescription %}{{ seo.openGraphDescription }}{% endblock %}"/>
  <link rel="shortcut icon" href="/favicon.ico">
  <link rel="home" href="{{ siteUrl }}" />
  {% block canonical %}{% endblock %}
  <link rel="stylesheet" href="/src/css/compiled.css">
</head>
<body>
  <div id="wrap">
  {% include "_includes/header" %}
  {% block content %}{% endblock %}
  {% include "_includes/footer" %}
  </div>
  <script src="/components/requirejs/require.js" data-main="/src/js/main.js"></script>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-21431284-3', 'auto');
    ga('send', 'pageview');

  </script>
</body>
</html>{% endcache %}
